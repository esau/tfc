<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns="http://www.owl-ontologies.com/OntologyTwitter.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.owl-ontologies.com/OntologyTwitter.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Media">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Entity"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Hashtag"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Url"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="UserMention"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Place">
    <owl:disjointWith>
      <owl:Class rdf:ID="Tweet"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="User"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Entity"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Coordinates"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Entity">
    <owl:disjointWith>
      <owl:Class rdf:about="#Tweet"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#User"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Place"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Coordinates"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#Url">
    <owl:disjointWith>
      <owl:Class rdf:about="#Hashtag"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Media"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#UserMention"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#Coordinates">
    <owl:disjointWith>
      <owl:Class rdf:about="#Tweet"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#User"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Entity"/>
    <owl:disjointWith rdf:resource="#Place"/>
  </owl:Class>
  <owl:Class rdf:about="#UserMention">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Hashtag"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Media"/>
    <owl:disjointWith rdf:resource="#Url"/>
  </owl:Class>
  <owl:Class rdf:about="#Hashtag">
    <rdfs:subClassOf rdf:resource="#Entity"/>
    <owl:disjointWith rdf:resource="#Media"/>
    <owl:disjointWith rdf:resource="#Url"/>
    <owl:disjointWith rdf:resource="#UserMention"/>
  </owl:Class>
  <owl:Class rdf:about="#Tweet">
    <owl:disjointWith>
      <owl:Class rdf:about="#User"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Entity"/>
    <owl:disjointWith rdf:resource="#Place"/>
    <owl:disjointWith rdf:resource="#Coordinates"/>
  </owl:Class>
  <owl:Class rdf:about="#User">
    <owl:disjointWith rdf:resource="#Coordinates"/>
    <owl:disjointWith rdf:resource="#Tweet"/>
    <owl:disjointWith rdf:resource="#Entity"/>
    <owl:disjointWith rdf:resource="#Place"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="inPlace">
    <rdfs:range rdf:resource="#Place"/>
    <rdfs:domain rdf:resource="#Coordinates"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="boundingBox"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isFollowedBy">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="follows"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#User"/>
    <rdfs:domain rdf:resource="#User"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="mentionsUser">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="mentionedIn"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#UserMention"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      The User mentioned&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="#User"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="inTweet">
    <rdfs:domain rdf:resource="#Coordinates"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasCoordinates"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="tweetRelated">
    <rdfs:domain rdf:resource="#Place"/>
    <rdfs:range rdf:resource="#Tweet"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="relatedToPlace"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasCoordinates">
    <owl:inverseOf rdf:resource="#inTweet"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Geographic location of this Tweet as reported by the User&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="#Coordinates"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#follows">
    <rdfs:range rdf:resource="#User"/>
    <rdfs:domain rdf:resource="#User"/>
    <owl:inverseOf rdf:resource="#isFollowedBy"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isAuthor">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="authoredBy"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#Tweet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="inReplyOfUser">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      If Tweet is a reply, contains the Tweet replied&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#User"/>
    <rdfs:domain rdf:resource="#Tweet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="comesFrom">
    <rdfs:range rdf:resource="#Tweet"/>
    <rdfs:domain rdf:resource="#Entity"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasEntities"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasUrl">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="comesFromUser"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Url"/>
    <rdfs:domain rdf:resource="#User"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#authoredBy">
    <owl:inverseOf rdf:resource="#isAuthor"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      User who authored this Tweet&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#User"/>
    <rdfs:domain rdf:resource="#Tweet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#mentionedIn">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="#UserMention"/>
    <owl:inverseOf rdf:resource="#mentionsUser"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#comesFromUser">
    <owl:inverseOf rdf:resource="#hasUrl"/>
    <rdfs:range rdf:resource="#User"/>
    <rdfs:domain rdf:resource="#Url"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="inReplyOfTweet">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      If Tweet is a reply, contains the Tweet replied&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#Tweet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#relatedToPlace">
    <rdfs:range rdf:resource="#Place"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      The Tweet is related to this Place&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <owl:inverseOf rdf:resource="#tweetRelated"/>
    <rdfs:domain rdf:resource="#Tweet"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#boundingBox">
    <owl:inverseOf rdf:resource="#inPlace"/>
    <rdfs:range rdf:resource="#Coordinates"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Coordinates making the bounding box that encloses the Place&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#Place"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasEntities">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Entities parsed out from the text of the Tweet&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="#Entity"/>
    <owl:inverseOf rdf:resource="#comesFrom"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isRetweet">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      If this Tweet is a retweet, contains the original Tweet&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="#Tweet"/>
    <rdfs:domain rdf:resource="#Tweet"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="tweetCount">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Number of Tweet published by User&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="attributes">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Place"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Additional information about the Place, like address etc.&#xD;
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="listedCount">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Number of public lists where this User is member&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="type">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Media"/>
          <owl:Class rdf:about="#Place"/>
          <owl:Class rdf:about="#Coordinates"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="friendsCount">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Number of User this User is following&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#User"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="timeZone">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      User defined time zone&#xD;
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="id">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Tweet"/>
          <owl:Class rdf:about="#User"/>
          <owl:Class rdf:about="#Media"/>
          <owl:Class rdf:about="#Place"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Unique id of this Individual
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="countryName">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Place's country name&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Place"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="indexA">
    <rdfs:domain rdf:resource="#Entity"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Initial position of the entity in the text field&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="screenName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      User pseudonym&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#User"/>
          <owl:Class rdf:about="#UserMention"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="description">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      User defined description&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="protected">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      has the User protected his tweets?&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="#User"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="location">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      User location, defined by himself&#xD;
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="createdAt">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Tweet"/>
          <owl:Class rdf:about="#User"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      UTC time of the creation of the Individual
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="longitude">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      longitude value for Coordinates&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#Coordinates"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="countryCode">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Place's country code&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#Place"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="mediaUrl">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Media"/>
          <owl:Class rdf:about="#Url"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      URL unique for this resourse.&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="fullName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Place"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="favouritesCount">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Tweet"/>
          <owl:Class rdf:about="#User"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Times this Tweet is favourited&#xD; or this User has favorited
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="displayUrl">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Url as it appears in the text field&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Media"/>
          <owl:Class rdf:about="#Url"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="lang">
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">machine detected language of the text</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="url">
    <rdfs:domain rdf:resource="#Place"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="indexB">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Final position of the entity text in the text field.&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#Entity"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isVerified">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Is User a verified individual?&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="name">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      User's name&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="followersCount">
    <rdfs:domain rdf:resource="#User"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      Number of Users following this User&#xD;
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="source">
    <rdfs:domain rdf:resource="#Tweet"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">Utility used to post the Tweet, HTML-formatted string.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="latitude">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#Coordinates"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      latitude vale of the Coordinates&#xD;
    &lt;/p&gt;</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="retweetsCount">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      number of times this Tweet has been retweetted&#xD;
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain rdf:resource="#Tweet"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="text">
    <rdfs:comment xml:lang="en">&lt;p style="margin-top: 0"&gt;&#xD;
      text of the individual
    &lt;/p&gt;</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Tweet"/>
          <owl:Class rdf:about="#Hashtag"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <UserMention rdf:ID="UserMentionAtoB">
    <indexB rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
    >44</indexB>
    <indexA rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
    >39</indexA>
    <mentionsUser>
